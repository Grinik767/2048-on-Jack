class Tile {
    field int value;

    method int getValue () {
        return value;
    }

    constructor Tile new(int nValue) {
        let value = nValue;
        return this;
    }

    method Tile Add (Tile other) {
        var int x;
        let x = other.getValue() + value;
        do other.dispose();
        do dispose();
        return Tile.new(x);
    }

    method void dispose () {
        do Memory.deAlloc(this);
        return;
    }
}