class Tile {
    field int value;

    method int getValue () {
        return value;
    }

    constructor Tile new(int nValue) {
        let value = nValue;
        return this;
    }

    method Tile Add (Tile other) {
        return Tile.new(other.getValue() + value);
    }

    method void dispose () {
        do Memory.deAlloc(this);
        return;
    }
}